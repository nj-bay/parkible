<h1> Create a new Parking Space </h1><br><br>

<%= provide :title, "Post a Space for Rent" %>

<%= form_for @space do |f| %>

	<%= f.label :address %>
	<%= f.text_field :address %>
<br><br>
	<%= f.label :price %>
	<%= f.text_field :price %><br><br>
	<%= f.label :image %>
	<%= f.file_field :image %> 

<br><br>
	<%= f.submit "Add", :class => 'btn btn-primary' %>	

<% end %>	